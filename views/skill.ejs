<section class="skills section" id="skills">
  <h2 class="section-title">Skills</h2>

  <div class="skills__container bd-grid">
    <div>
      <h2 class="skills__subtitle">Profesional Skills</h2>
      <p class="skills__text">
        Lorem ipsum dolor sit, amet consectetur adipisicing elit. Velit optio id
        vero amet, alias architecto consectetur error eum eaque sit.
      </p>
      <div class="skills__data">
        <div class="skills__names">
          <i class="bx bxl-html5 skills__icon"></i>
          <span class="skills__name"> <%=skill[0].nameSkill%></span>
        </div>
        <div class="skills__bar skills__html"></div>
        <div>
          <span class="skills__percentage"><%=skill[0].percentSkill%></span>
        </div>
      </div>
      <div class="skills__data">
        <div class="skills__names">
          <i class="bx bxl-css3 skills__icon"></i>
          <span class="skills__name"><%=skill[1].nameSkill%></span>
        </div>
        <div class="skills__bar skills__css"></div>
        <div>
          <span class="skills__percentage"><%=skill[1].percentSkill%></span>
        </div>
      </div>
      <div class="skills__data">
        <div class="skills__names">
          <i class="bx bxl-javascript skills__icon"></i>
          <span class="skills__name"><%=skill[2].nameSkill%></span>
        </div>
        <div class="skills__bar skills__js"></div>
        <div>
          <span class="skills__percentage"><%=skill[2].percentSkill%></span>
        </div>
      </div>
      <div class="skills__data">
        <div class="skills__names">
          <i class="bx bxs-paint skills__icon"></i>
          <span class="skills__name"><%=skill[3].nameSkill%></span>
        </div>
        <div class="skills__bar skills__ux"></div>
        <div>
          <span class="skills__percentage"><%=skill[3].percentSkill%></span>
        </div>
      </div>
    </div>

    <div>
      <img src="./img/bull-removebg-preview.png" alt="" class="skills__img" />
    </div>
  </div>
</section>
